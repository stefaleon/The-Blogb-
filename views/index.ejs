<% include ./partials/header %>

		<div class="ui container left aligned" id="content">

			<% posts.forEach(function (post) { %>
				<hr />
				<h2> <%= post.title %> </h2>
				<p><img src="<%= post.image %>" /></p>
				<p><span> <%= post.created %> </span></p>
				<p> <%= post.body %> </p>
			<% }); %>

			
		</div> <!-- container --> 

<% include ./partials/footer %>